   -
     name : LockOnFile
     internal_properties : CAAAAAgAAAC6lkRD7wAlEubxavA0zFuDccYLRZteszRaUxPzcwMrWcQnQoLyZ2Bp1Jz29yBlhwy7+1gKc5cU5qdnBEA/HaahP+uzkMxap/ROPskLyd8UG2H8SHu04t/6+gVlBDF++ndx4PO8ybb6+k7HEE+MBWjpnC4xpFEaAIsYQ4vKbvmxSqh/jkGJwTlU5/YQ4WF0j+gDukY/
     procedure_id : 1762245830556900445
     type_code : 14
     code : |1+
      PROCEDURE LockOnFile()
      
      i is int = 0
      
      // retrieves information about the lock
      sLockInfo is string = HErrorInfo(hErrLockInfo)
      
      // if lock information is found
      IF sLockInfo <> "" THEN
      	// formatting the caption
      	STC_Explanations = StringBuild(<§$0008§>,HErrorInfo(hErrFile))	
      	
      	sMsg is string
      	sOneLock is string = ExtractString(sLockInfo, firstRank, CR)
      	WHILE sOneLock <> EOT
      		i++
      		sMsg += StringBuild(<§$0009§>+CR,ExtractString(sOneLock, 3, TAB),ExtractString(sOneLock, 6, TAB),ExtractString(sOneLock, 4, TAB))
      		sOneLock = ExtractString(sLockInfo, nextRank, CR)
      	END
      	IF Right(sMsg,2) = CR THEN sMsg = sMsg[[ TO Length(sMsg)-2 ]]
      	nHeight is int
      
      	// if there is a single row
      	IF i=1 THEN
      		// deletes '- '
      		sMsg = sMsg[[ 3 TO Length(sMsg) ]]
      		// defines the height of the window according to the lock information
      		nHeight = TextHeight(ListLocks,sMsg,tMultiline)
      		nHeight = Min(nHeight,MAXHEIGHT)
      		MySelf..Height += nHeight
      
      		ListLocks = sMsg
      		ListLocks..Visible = True
      	ELSE
      		// defines the height of the window according to the lock information
      		nHeight = TextHeight(ListMultiLocks,sMsg)
      		nHeight = Min(nHeight,MAXHEIGHT)
      		IF nHeight = MAXHEIGHT THEN ScrollBarShow(ListMultiLocks,sbVert,True)
      		IF TextWidth(ListMultiLocks,sMsg)>ListMultiLocks..Width THEN
      			ScrollBarShow(ListMultiLocks,sbHorz,True)
      			// add the scrollbar height + 5 pixels in order for the text not to be stuck to the scrollbar
      			nHeight = nHeight+SysMetric(metCyHScroll)+5
      		END
      		MySelf..Height += nHeight
      
      		ListMultiLocks = sMsg
      		ListMultiLocks..Visible = True
      	END
      ELSE	
      	// formatting the caption
      	STC_Explanations = <§$0003§>+HErrorInfo(hErrFile)+<§$0004§>
      END
     type : 458752
   -
     name : LockOnDatabase
     procedure_id : 1762245830556965981
     type_code : 14
     code : |1+
      PROCEDURE LockOnDatabase()
      
      // specify the nature of the lock
      Header = <§$0010§>
      
      // retrieves the information about the database lock
      sLockInfo is string = HErrorInfo(hErrLockInfo)
      
      // retrieves the name of the database
      sDatabaseName is string = ExtractString(sLockInfo, 1, TAB)
      
      // retrieves the name of the computer and the name of the user who locked the database
      sLogin is string = ExtractString(sLockInfo, 2, TAB)
      sMachine is string = ExtractString(sLockInfo, 3, TAB)
      
      // if user information and computer information are found
      IF sLogin <> EOT _AND_ sMachine <> EOT THEN
      	// formatting the caption
      	STC_Explanations = StringBuild(<§$000b§>,sDatabaseName, sLogin, sMachine )
      ELSE	
      	// formatting the caption
      	STC_Explanations = <§$000c§>
      END
     type : 458752
