   -
     code : |1+
      // retrieves the full error message regarding the problem that occurred
      gsDetails is string = HErrorInfo(hErrFullDetails)
      
      CONSTANT MAXHEIGHT=200
      
      // title of the window = name of the application
      MySelf..Title = fExtractPath(ExeInfo(exeName),fFileName)
      // Return value by default when closing without using the buttons
      MySelf..ReturnedValue=opCancel
      
      // if the database is locked
      IF HError() = 74011 THEN
      	LockOnDatabase()
      ELSE 
      	// it's a lock on a file
      	LockOnFile()
      END
      
      
